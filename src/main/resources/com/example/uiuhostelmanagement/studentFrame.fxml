<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="rootPane" prefHeight="684.0" prefWidth="1024.0" stylesheets="@../admin.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.uiuhostelmanagement.StudentDashboad">

    <right>
        <VBox prefHeight="539.0" prefWidth="156.0" BorderPane.alignment="CENTER">
            <children>

                <Button fx:id="addMealButton" contentDisplay="TOP" mnemonicParsing="false" onAction="#addMealButtonAction" prefHeight="400.0" prefWidth="156.0" text="Add Meal">

                </Button>

                <Button fx:id="recieveBillButton" contentDisplay="TOP" layoutX="10.0" layoutY="266.0" mnemonicParsing="false" onAction="#billRecive" prefHeight="400.0" prefWidth="156.0" text="Recieve Bill">

                </Button>
                <Button fx:id="dailyMealBillsButton" contentDisplay="TOP" layoutX="10.0" layoutY="448.0" mnemonicParsing="false" onAction="#dailyMealBillsButtonAction" prefHeight="400.0" prefWidth="156.0" text="Meal Details">
                    <graphic>

                    </graphic>
                </Button>


                <Button fx:id="logoutButton" contentDisplay="TOP" layoutX="10.0" layoutY="422.0" mnemonicParsing="false" onAction="#logoutButtonAction" prefHeight="400.0" prefWidth="156.0" text="Log Out">
                    <graphic>

                    </graphic>
                </Button>
            </children>
        </VBox>
    </right>
   <center>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <ButtonBar prefHeight="50.0" prefWidth="854.0">
               <buttons>
                  <Button mnemonicParsing="false" onAction="#dashboad" prefHeight="8.0" prefWidth="328.0" text="Dashboad" />
                  <Button mnemonicParsing="false" onAction="#notices" text="Notices" />
                  <Button mnemonicParsing="false" onAction="#Complain" text="Complains" />
               </buttons>
            </ButtonBar>
            <BorderPane layoutY="61.0" prefHeight="623.0" prefWidth="942.0" />
         </children>
      </AnchorPane>
   </center>

</BorderPane>
